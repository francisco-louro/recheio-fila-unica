<?xml version='1.0' encoding='utf-8'?>
<widget id="pt.ztech.recheio" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Recheio Fila Unica</name>
    <description>
        Recheio Fila Unica
    </description>
    <author email="info@ztech.pt" href="http://www.ztech.pt">
        Ztech Team
    </author>
    <content src="index.html" />

    <preference name="android-minSdkVersion" value="10" />
    <preference name="android-maxSdkVersion" value="" />
    <!--<preference name="android-targetSdkVersion" value="22" />-->
	
    <!-- SEM RESTRIÇÕES DE IP -->
	<access origin="*" />
    <allow-intent href="*" />   
    <allow-navigation href="*" />
	
	<!-- COM RESTRIÇÕES DE IP -->
	<!--<access origin="*://10.133.37.201/*" />
    <allow-intent href="*://10.133.37.201/*" />   
    <allow-navigation href="*://10.133.37.201/*" />-->
	
	<!--<access origin="*://192.168.1.108/*" />
    <allow-intent href="*://192.168.1.108/*" />   
    <allow-navigation href="*://192.168.1.108/*" />-->
	
    <plugin name="cordova-plugin-whitelist" spec="1.3.4" />
    <plugin name="cordova-plugin-splashscreen" spec="5.0.2" />
	
    
    <preference name="SplashScreenDelay" value="5000" />
    <preference name="orientation" value="all" />
    <preference name="fullscreen" value="true" />
    <preference name="KeepRunning" value="false"/>
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="ShowSplashScreenSpinner" value="false"/>
    <preference name="SplashScreen" value="screens" />
    <preference name="SplashMaintainAspectRatio" value="true" /> 
	
    <icon src="icon.png" />
    <splash src="splash.png" />
    <platform name="android">
        <icon qualifier="ldpi" src="www/res/icons/android/drawable-ldpi-icon.png" />
        <icon qualifier="mdpi" src="www/res/icons/android/drawable-mdpi-icon.png" />
        <icon qualifier="hdpi" src="www/res/icons/android/drawable-hdpi-icon.png" />
        <icon qualifier="xhdpi" src="www/res/icons/android/drawable-xhdpi-icon.png" />
        <icon qualifier="xxhdpi" src="www/res/icons/android/drawable-xxhdpi-icon.png" />
        <icon qualifier="xxxhdpi" src="www/res/icons/android/drawable-xxxhdpi-icon.png" />
        <splash qualifier="land-ldpi" src="www/res/screens/android/drawable-land-ldpi-screen.png" />
        <splash qualifier="land-mdpi" src="www/res/screens/android/drawable-land-mdpi-screen.png" />
        <splash qualifier="land-hdpi" src="www/res/screens/android/drawable-land-hdpi-screen.png" />
        <splash qualifier="land-xhdpi" src="www/res/screens/android/drawable-land-xhdpi-screen.png" />
        <splash qualifier="land-xxhdpi" src="www/res/screens/android/drawable-land-xxhdpi-screen.png" />
        <splash qualifier="land-xxxhdpi" src="www/res/screens/android/drawable-land-xxxhdpi-screen.png" />
        <splash qualifier="port-ldpi" src="www/res/screens/android/drawable-port-ldpi-screen.png" />
        <splash qualifier="port-mdpi" src="www/res/screens/android/drawable-port-mdpi-screen.png" />
        <splash qualifier="port-hdpi" src="www/res/screens/android/drawable-port-hdpi-screen.png" />
        <splash qualifier="port-xhdpi" src="www/res/screens/android/drawable-port-xhdpi-screen.png" />
        <splash qualifier="port-xxhdpi" src="www/res/screens/android/drawable-port-xxhdpi-screen.png" />
        <splash qualifier="port-xxxhdpi" src="www/res/screens/android/drawable-port-xxxhdpi-screen.png" />
    </platform>
    <platform name="ios">
        <icon height="57" platform="ios" src="www/res/icons/ios/icon.png" width="57" />
        <icon height="114" platform="ios" src="www/res/icons/ios/icon@2x.png" width="114" />
        <icon height="40" platform="ios" src="www/res/icons/ios/icon-40.png" width="40" />
        <icon height="80" platform="ios" src="www/res/icons/ios/icon-40@2x.png" width="80" />
        <icon height="50" platform="ios" src="www/res/icons/ios/icon-50.png" width="50" />
        <icon height="100" platform="ios" src="www/res/icons/ios/icon-50@2x.png" width="100" />
        <icon height="60" platform="ios" src="www/res/icons/ios/icon-60.png" width="60" />
        <icon height="120" platform="ios" src="www/res/icons/ios/icon-60@2x.png" width="120" />
        <icon height="180" platform="ios" src="www/res/icons/ios/icon-60@3x.png" width="180" />
        <icon height="72" platform="ios" src="www/res/icons/ios/icon-72.png" width="72" />
        <icon height="144" platform="ios" src="www/res/icons/ios/icon-72@2x.png" width="144" />
        <icon height="76" platform="ios" src="www/res/icons/ios/icon-76.png" width="76" />
        <icon height="152" platform="ios" src="www/res/icons/ios/icon-76@2x.png" width="152" />
        <icon height="29" platform="ios" src="www/res/icons/ios/icon-small.png" width="29" />
